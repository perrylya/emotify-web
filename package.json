{
  "name": "react-node-starter",
  "version": "1.0.0",
  "description": "an example for deploying a React + NodeJS app to Heroku",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack -d --watch",
    "backend": "nodemon server.js",
    "start": "npm run build && NODE_ENV=production node server.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.js --require ignore-styles"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async-request": "^1.2.0",
    "axios": "^0.18.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "base64-img": "^1.0.4",
    "body-parser": "^1.18.3",
    "cookie-parser": "1.3.2",
    "cors": "^2.8.4",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.1",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "face-plus-plus": "0.0.3",
    "file-system": "^2.2.2",
    "form-data": "^2.3.2",
    "fs": "0.0.1-security",
    "mocha": "^3.2.0",
    "node": "^10.6.0",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.5.2",
    "prop-types": "^15.5.8",
    "querystring": "~0.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-spring": "^5.4.0",
    "react-webcam": "^1.0.1",
    "request": "~2.83.0",
    "request-promise": "^4.2.2",
    "sass-loader": "^6.0.3",
    "save": "^2.3.2",
    "semantic-ui": "^2.3.3",
    "semantic-ui-react": "^0.82.0",
    "style-it": "^2.0.0",
    "style-loader": "^0.16.1",
    "styled-components": "^3.3.3",
    "webpack": "^2.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
